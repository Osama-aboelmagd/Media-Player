<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.osama.mediaplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Search"
                  android:label="@string/search_label"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AllSong"
            android:label="@string/All_Song" />
        <activity
            android:name=".Arabic"
            android:label="@string/Arabic" />
        <activity
            android:name=".English"
            android:label="@string/English" />
        <activity
            android:name=".Classic"
            android:label="@string/Classic" />

    </application>

</manifest>